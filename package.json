  },
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/asciichart": "^1.5.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "asciichart": "^1.5.25",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0"
