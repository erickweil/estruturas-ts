  "version": "0.1.0",
  "author": "Erick L. Weil",
  "license": "MIT",
  "keywords": [
    "typescript",
    "data-structures",
    "estruturas",
    "queue",
    "linked-list",
    "stack"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erickweil/estruturas-ts.git"
  },
  "engines": {
